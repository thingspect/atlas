            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "uniqID",
            "description": "Device unique ID.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "devID",
            "description": "Device ID (UUID).",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "attr",
            "description": "Device attribute. If not specified, all attributes are returned.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "endTime",
            "description": "Data points range end time. Defaults to current time if not specified. Maximum supported time range is 90 days.",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "startTime",
            "description": "Data points range start time. Defaults to end_time - 24 hours if not specified or after end_time. Maximum supported time range is 90 days.",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          }
        ],
        "tags": [
          "DataPointService"
        ]
